@{
}
@{
    ViewData["Title"] = "大數據房價分析!";
}
<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
<link rel="stylesheet" href="~/css/loading.css" />
<h3>@ViewData["Message"]</h3>
<div class="jumbotron">
    <h1>大數據房價分析! </h1>
    <h3>
        用最快速方式找到最適合的住宅 For Free! <a href="https://www.facebook.com/visualizationRealPrice/">FB 建議我!</a>
    </h3>
    <h3>Apple系列請透過Chrome查詢</h3>
</div>


<div class="panel panel-default">
    <div class="panel-heading">
        <div class="row">
            <div class="btn btn-group" role="group" aria-label="...">
                <button id="btnCity0" type="button" class="btn btn-default" onclick="SelectCity(0)">台北 新北</button>
                <button id="btnCity1" type="button" class="btn btn-default" onclick="SelectCity(1)">台中</button>
                <button id="btnCity2" type="button" class="btn btn-default" onclick="SelectCity(2)">高雄</button>
                <button id="btnCity3" type="button" class="btn btn-default" onclick="SelectCity(3)">新竹</button>
                <button id="btnCity4" type="button" class="btn btn-default" onclick="SelectCity(4)">桃園</button>
                <button id="btnCity" type="button" class="btn btn-default">請點選下圖!!!</button>
            </div>
        </div>
    </div>

    <div class="panel-body">
        <div class="row">
            <div id="vizTreeMap" class="col-md-12" style="height:400px"></div>
        </div>
        <br />
        <div class="row">
            <nav class="navbar navbar-default">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <a class="navbar-brand" href="#">排序!</a>
                    </div>
                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                        <form class="navbar-form navbar-left">
                            <div class="form-group">
                                <button id="btnByTprice" type="button" class="btn btn-default" onclick="orderDataDesc('tprice')">總價</button>
                                <button id="btnByLocation" type="button" class="btn btn-default" onclick="orderDataDesc('uprice')">每坪價格</button>
                                <button id="btnByLocation" type="button" class="btn btn-default" onclick="orderDataDesc('houseage')">房齡</button>
                                <button id="btnByLocation" type="button" class="btn btn-default" onclick="orderDataDesc('location')">地點</button>
                            </div>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                        </form>

                    </div><!-- /.navbar-collapse -->
                </div><!-- /.container-fluid -->
            </nav>


        </div>
        <div class="row">
            <div id="vizHCLocation" class="col-md-12" style="min-height:300px"></div>
        </div>
        <br />

        <div class="row">
            <div class="col-md-6 col-sm-6">
                <div id="vizHistory" class="col-md-12" style="min-height:220px"></div>
            </div>
            <div class="col-md-6 col-sm-6">
                <div id="map" class="col-md-12" style="min-height:220px"></div>
            </div>

        </div>
        <br />

    </div>

</div>

<script src="~/lib/jquery/dist/jquery.js"></script>

